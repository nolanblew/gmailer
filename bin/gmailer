require 'gmailer'
first_rg = ARGV[0]

if (first_rg == "generate")
	puts Gmailer.generate(ARGV[1], ARGV[2])
elsif (first_rg == "help")
	puts "Commands:"
	puts "gmailer generate [username] [password]"
	puts "\tusername: your gmail username"
	puts "\tpassword: your gmail password"
else
	puts "Error, unrecognized command. Please type in 'gmailer help' for help."
end